rm(list=ls())
library("readxl")
library("bruceR")
library("rstatix")
library("car")
data<-read_excel("hw8_data.xlsx")
data$method<-as.factor(data$method)
shapiro_test(data$score[data$method == 0])
shapiro_test(data$score[data$method == 1])
shapiro_test(data$score[data$size == "large"])
shapiro_test(data$score[data$size == "medium"])
shapiro_test(data$score[data$size == "small"])
leveneTest(score~method*size,data)
m<-MANOVA(data=data,dv="score",between=c("method","size"))
EMMEANS(m,effect = "size",p.adjust="tukey")
EMMEANS(m,effect = "size",by="method",p.adjust="bonferroni")
emmip(m,method ~ size)
