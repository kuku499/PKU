rm(list=ls())
library(readxl)
library(rstatix)
library(stats)
library(dplyr)
data<-read_excel("hw_11.xlsx")
attach(data)
plot(SleepHours,ReactionTime)
data%>%
group_by()%>%
shapiro_test(SleepHours,ReactionTime)
cor.test(SleepHours,ReactionTime)
lm_results=lm(ReactionTime~SleepHours,data=data)
summary(lm_results)
plot(SleepHours,ReactionTime,abline(lm_results))
predict(lm_results)
new_data<-data.frame(SleepHours=c(6.5,8.0))
predict(lm_results,newdata = new_data,interval = "confidence")
